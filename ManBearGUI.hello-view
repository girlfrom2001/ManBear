               <Button fx:id="grid" maxWidth="100.0" mnemonicParsing="false" onAction="#Grey" prefHeight="170.0" style="-fx-background-color: #ffffff;" GridPane.columnIndex="1" />
               <Button defaultButton="true" mnemonicParsing="false" prefHeight="100.0" prefWidth="170.0" style="-fx-background-color: #808080;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <Button fx:id="grid" mnemonicParsing="false" onAction="#Grey" prefHeight="100.0" prefWidth="170.0" style="-fx-background-color: #ffffff;" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               <Button fx:id="grid" mnemonicParsing="false" onAction="#Grey" prefHeight="100.0" prefWidth="170.0" style="-fx-background-color: #ffffff;" GridPane.columnIndex="2" />
               <Button fx:id="grid" mnemonicParsing="false" onAction="#Grey" prefHeight="100.0" prefWidth="170.0" style="-fx-background-color: #ffffff;" GridPane.columnIndex="2" GridPane.rowIndex="1" />
               <Button fx:id="grid" mnemonicParsing="false" onAction="#Grey" prefHeight="100.0" prefWidth="170.0" style="-fx-background-color: #ffffff;" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            </children>
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin>
