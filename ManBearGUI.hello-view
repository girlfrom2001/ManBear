          <Button contentDisplay="CENTER" onAction="#Attack" text="Attack">
            <HBox.margin>
               <Insets right="50.0" />
            </HBox.margin></Button>
         <Button fx:id="healButton" alignment="CENTER_RIGHT" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" onAction="#Heal" text="Heal">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin></Button>
      </children>
   </HBox>
